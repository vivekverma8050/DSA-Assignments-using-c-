#include <stdio.h>
#include <limits.h>

void findSecondMinimum(int arr[], int n) {
    if (n < 2) {
        printf("Array must contain at least two elements.\n");
        return;
    }

    int first = INT_MAX, second = INT_MAX;

    for (int i = 0; i < n; i++) {
        if (arr[i] < first) {
            second = first;
            first = arr[i];
        } else if (arr[i] < second && arr[i] != first) {
            second = arr[i];
        }
    }

    if (second == INT_MAX) {
        printf("No second minimum element found (all elements might be equal).\n");
    } else {
        printf("The second minimum element is %d\n", second);
    }
}

int main() {
    int arr[] = {70, 40, 30, 20, 10, 90};
    int n = sizeof(arr) / sizeof(arr[0]);

    findSecondMinimum(arr, n);

    return 0;
}
