#include <stdio.h>

void rotateArr(int arr[], int d, int n) {
    d %= n; // Handle if d > n
    int temp[n];

    // Copy the elements from d to end
    for (int i = 0; i < n - d; i++)
        temp[i] = arr[d + i];

    // Copy the first d elements to the end
    for (int i = 0; i < d; i++)
        temp[n - d + i] = arr[i];

    // Copy temp back to original array
    for (int i = 0; i < n; i++)
        arr[i] = temp[i];
}

int main() {
    int arr[] = {1, 2, 3, 4, 5, 6};
    int n = sizeof(arr) / sizeof(arr[0]);
    int d = 2; // Number of rotations

    rotateArr(arr, d, n);

    // Print rotated array
    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
